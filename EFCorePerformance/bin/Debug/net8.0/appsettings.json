{
  "ConnectionStrings": {
    //"Database": "Server=localhost;Database=efperformance;Trusted_Connection=True;",
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=efperformance;Trusted_Connection=True;MultipleActiveResultSets=true"
